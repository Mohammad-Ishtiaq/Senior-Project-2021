<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             NavigationPage.HasNavigationBar="false"
             BackgroundImageSource="Background.jpg"
             x:Class="SpFinal.MainPage">

    <StackLayout HorizontalOptions="Center" Padding="10,381,0,0">

        <Button x:Name="symptomChecker"
                Clicked="symptomChecker_Clicked"
                HeightRequest="50"
                WidthRequest="10"
                Text="Symptom Checker"/>
        <Label Padding="0,15,0,0"/>

        <Button x:Name="covidTracker"
                Clicked="covidTracker_Clicked"
                Text="Covid Tracker"/>

        <Label/>

        <Button x:Name="Admin"
                Clicked="Admin_Clicked"
                Text="Admin Panel"/>
    </StackLayout>

</ContentPage>
