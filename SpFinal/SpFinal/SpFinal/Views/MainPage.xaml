<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             NavigationPage.HasNavigationBar="false"
             BackgroundImageSource="Background.jpg"
             x:Class="SpFinal.MainPage">

    <StackLayout HorizontalOptions="Center" VerticalOptions="Center" Padding="30,0,0,0">
        <Label Padding="0,0,0,220"/>
        
        <Button x:Name="symptomChecker"
                Clicked="symptomChecker_Clicked"
                Text="Symptom Checker"/>
        
        <Label Padding="0,0,0,15"/>
        
        <Button x:Name="covidTracker"
                Clicked="covidTracker_Clicked"
                Text="Covid Tracker"/>
    </StackLayout>

</ContentPage>
