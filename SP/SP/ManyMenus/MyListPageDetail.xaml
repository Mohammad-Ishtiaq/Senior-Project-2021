<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SP.ManyMenus.MyListPageDetail">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center" VerticalOptions="Center">
            <Image x:Name="MyImageCall" HeightRequest="200" WidthRequest="200"/>
            <Label x:Name="MyItemNameShow" HorizontalTextAlignment="Center" FontSize="Large"/>
            <Label x:Name="MyIngrediantItemShow" Margin="20,20" FontSize="Medium"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>